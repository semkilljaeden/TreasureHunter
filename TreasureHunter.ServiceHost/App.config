<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="Couchbase" type="Couchbase.Configuration.Client.Providers.CouchbaseClientSection, Couchbase.NetClient" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2" />
  </startup>
  <Couchbase useSsl="false" operationLifeSpan="1000">
    <servers>
      <add uri="http://sd-09d2-0287.nam.nsroot.net:8091/pools"></add>
      <add uri="http://localhost:8091/pools"></add>
    </servers>
    <buckets>
      <add name="TreasureHunter" useSsl="yes" password="test" operationLifespan="2000">
        <connectionPool name="custom" maxSize="10" minSize="5" sendTimeout="12000"></connectionPool>
      </add>
    </buckets>
  </Couchbase>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="log4net.Config" value="log.config" />
    <add key="log4net.Config.Watch" value="True" />
  </appSettings>
</configuration>
